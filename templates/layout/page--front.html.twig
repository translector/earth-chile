<div id="page-wrapper">
  <div id="page">



<!-- Header and Navbar -->
<header class="main-header">
  <!--Start: Branding and Menu -->
  <div class="menu">
    <div class="container">
      <div class="row">
        {% if page.header %}
          <div class="col-xs-6 col-md-3">
            {{ page.header }}
          </div>
        {% endif %}
        <!-- Navigation -->
        {% if page.menu %}
          <div class="col-xs-6 col-md-9">
            {{ page.menu }} 
          </div>
        {% endif %}
        <!--End Navigation -->
      </div>
    </div>
  </div>


  <!-- Start: Banner -->
    {% if page.welcome_banner_text %}
      <div class="container">
        <div class="banner">
          <div class="row">
            <div class="col-md-12">
              {{ page.welcome_banner_text }}
            </div>
          </div>
        </div>
      </div>
    {% endif %}
  <!-- End: Banner -->




  <!--Start: Highlighted -->
    {% if page.highlighted %}
      <div class="highlighted">
        <div class="container">
          <div class="row">
            <div class="col-md-12">
              {{ page.highlighted }}
            </div>
          </div>
        </div>
      </div>
    {% endif %}
  <!--End: Highlighted -->

</header>
<!--End Header & Navbar -->



